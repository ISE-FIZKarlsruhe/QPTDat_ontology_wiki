                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty qpt:hasApplicationField ;
                                                                 owl:hasValue qpt:gasMetalArcWelding_field
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty qpt:hasApplicationField ;
                                                                 owl:hasValue qpt:pulsedMagWelding_field
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty qpt:hasApplicationField ;
                                                                 owl:hasValue qpt:pulsedMigWelding_field
                                                               ]
                                                             ) ;
