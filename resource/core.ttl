#################################################################
#    Object Properties
#################################################################

###  http://orkg.org/core#utilizes
orkg:utilizes rdf:type owl:ObjectProperty .


###  http://orkg.org/core#yields
orkg:yields rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/contributor
dcterms:contributor rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/license
dcterms:license rdf:type owl:ObjectProperty .


###  http://purl.org/net/p-plan#hasInputVar
p-plan:hasInputVar rdf:type owl:ObjectProperty .


###  http://purl.org/net/p-plan#hasOutputVar
p-plan:hasOutputVar rdf:type owl:ObjectProperty .


###  http://purl.org/net/p-plan#isInputVarOf
p-plan:isInputVarOf rdf:type owl:ObjectProperty .


###  http://purl.org/net/p-plan#isOutputVarOf
p-plan:isOutputVarOf rdf:type owl:ObjectProperty .


###  http://purl.org/net/p-plan#isStepOfPlan
p-plan:isStepOfPlan rdf:type owl:ObjectProperty .


###  http://purl.org/net/p-plan#isSubPlanOfPlan
p-plan:isSubPlanOfPlan rdf:type owl:ObjectProperty .


###  http://qptdat.plasma-mds.org/ontology/author
:author rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf dcterms:contributor ;
        rdfs:domain :Dataset ;
        rdfs:range :Person ;
        rdfs:comment "A person, who created the dataset."@en ;
        rdfs:label "author"@en .


###  http://qptdat.plasma-mds.org/ontology/composedOf
:composedOf rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain :Material ;
            rdfs:range :ChemicalCompound ;
            rdfs:comment "A chemical compound, that is part of the material."@en ;
            rdfs:label "composed of"@en .


###  http://qptdat.plasma-mds.org/ontology/configuredBy
:configuredBy rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain :Device ;
              rdfs:range :Configuration ;
              owl:propertyChainAxiom ( :configuredBy
                                       :hasSubConfiguration
                                     ) ,
                                     ( :usesDevice
                                       :configuredBy
                                     ) ;
              rdfs:comment "A configuration of the device."@en ;
              rdfs:label "configured by"@en .


###  http://qptdat.plasma-mds.org/ontology/consistsOfResource
:consistsOfResource rdf:type owl:ObjectProperty ;
                    rdfs:domain :Dataset ;
                    rdfs:range :DataResource ;
                    rdfs:comment "Links a dataset to a data resource."@en ;
                    rdfs:label "consists of resource"@en .


###  http://qptdat.plasma-mds.org/ontology/contact
:contact rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf <http://www.w3.org/ns/prov#wasAttributedTo> ;
         rdfs:domain :Source ;
         rdfs:range <http://www.w3.org/ns/prov#Agent> ;
         rdfs:comment "A contact person/organization for the plasma source."@en ;
         rdfs:label "contact"@en .


###  http://qptdat.plasma-mds.org/ontology/contributor
:contributor rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf dcterms:contributor ;
             rdfs:domain :PlasmaStudy ;
             rdfs:range :Person ;
             rdfs:comment "A person, who contributed to the plasma study."@en ;
             rdfs:label "contributor"@en .


###  http://qptdat.plasma-mds.org/ontology/definedBy
:definedBy rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain :Material ;
           rdfs:range :MaterialProperty ;
           rdfs:comment "A property of the material."@en ;
           rdfs:label "defined by"@en .


###  http://qptdat.plasma-mds.org/ontology/hasAccessLevel
:hasAccessLevel rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:domain :Dataset ;
                rdfs:range :AccessLevel ;
                rdfs:comment "The access level of a dataset."@en ;
                rdfs:label "has access level"@en .


###  http://qptdat.plasma-mds.org/ontology/hasApplicationField
:hasApplicationField rdf:type owl:ObjectProperty ;
                     rdfs:domain :Source ;
                     rdfs:range :ResearchTopic ;
                     owl:propertyChainAxiom ( :hasApplicationField
                                              :isSubResearchTopicOf
                                            ) ;
                     rdfs:comment "A research topic where the plasma source is mostly used. "@en ;
                     rdfs:label "has application field"@en .


###  http://qptdat.plasma-mds.org/ontology/hasExperimentStep
:hasExperimentStep rdf:type owl:ObjectProperty ;
                   owl:inverseOf :isExperimentStepOf ;
                   rdfs:domain :PlasmaStudy ;
                   rdfs:range :ExperimentStep ;
                   rdfs:comment "A step of the plasma study."@en ;
                   rdfs:label "has experiment step"@en .


###  http://qptdat.plasma-mds.org/ontology/hasInputVar
:hasInputVar rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf orkg:utilizes ,
                                p-plan:hasInputVar ;
             owl:inverseOf :isInputVarOf ;
             rdfs:domain :ExperimentStep ;
             rdfs:range :Variable ;
             owl:propertyChainAxiom ( :hasInputVar
                                      :configuredBy
                                      :usesMedium
                                    ) ;
             rdfs:comment "A input variable of the step"@en ;
             rdfs:label "has input variable"@en .


###  http://qptdat.plasma-mds.org/ontology/hasOutputVar
:hasOutputVar rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf p-plan:hasOutputVar ;
              owl:inverseOf :isOutputVarOf ;
              rdfs:domain :ExperimentStep ;
              rdfs:range :Variable ;
              rdfs:comment "A output variable of the step."@en ;
              rdfs:label "has output variable"@en .


###  http://qptdat.plasma-mds.org/ontology/hasPlasmaStudyTag
:hasPlasmaStudyTag rdf:type owl:ObjectProperty ;
                   rdfs:domain :PlasmaStudy ;
                   rdfs:range :PlasmaStudyTag ;
                   owl:propertyChainAxiom ( :hasPlasmaStudyTag
                                            :isSubPlasmaStudyTagOf
                                          ) ;
                   rdfs:comment "A closed vocabulary of keywords to mark plasma studies, like basic research, etc. "@en ;
                   rdfs:label "has plasma study tag"@en .


###  http://qptdat.plasma-mds.org/ontology/hasRange
:hasRange rdf:type owl:ObjectProperty ;
          rdfs:domain :NumericalConfiguration ;
          rdfs:range :Range ;
          rdfs:comment "A range of values for the numerical configuration."@en ;
          rdfs:label "has range"@en .


###  http://qptdat.plasma-mds.org/ontology/hasResearchTopic
:hasResearchTopic rdf:type owl:ObjectProperty ;
                  rdfs:domain :PlasmaStudy ;
                  rdfs:range :ResearchTopic ;
                  owl:propertyChainAxiom ( :hasResearchTopic
                                           :isSubResearchTopicOf
                                         ) ;
                  rdfs:comment "A topic related to the plasma study."@en ;
                  rdfs:label "has research topic"@en .


###  http://qptdat.plasma-mds.org/ontology/hasSubConfiguration
:hasSubConfiguration rdf:type owl:ObjectProperty ;
                     owl:inverseOf :isSubConfigurationOf ;
                     rdfs:domain :Configuration ;
                     rdfs:range :Configuration ;
                     rdfs:comment "A more specific configuration."@en ;
                     rdfs:label "has subconfiguration"@en .


###  http://qptdat.plasma-mds.org/ontology/hasSubMaterialProperty
:hasSubMaterialProperty rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf owl:topObjectProperty ;
                        owl:inverseOf :isSubMaterialPropertyOf ;
                        rdfs:domain :MaterialProperty ;
                        rdfs:range :MaterialProperty ;
                        rdfs:comment "A more specific material property."@en ;
                        rdfs:label "has submaterial property"@en .


###  http://qptdat.plasma-mds.org/ontology/identifier
:identifier rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain :Dataset ;
            rdfs:range :Doi ;
            rdfs:comment "identifies the dataset to an Digital Object Identifier (DOI), and Uniform Resource Name (URN). Persistent identifiers should be provided as HTTP URIs."@en ;
            rdfs:label "identifier"@en .


###  http://qptdat.plasma-mds.org/ontology/isExperimentStepOf
:isExperimentStepOf rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf p-plan:isStepOfPlan ;
                    rdfs:domain :ExperimentStep ;
                    rdfs:range :PlasmaStudy ;
                    rdfs:comment "A step of the plasma study."@en ;
                    rdfs:label "is experiment step of"@en .


###  http://qptdat.plasma-mds.org/ontology/isInputVarOf
:isInputVarOf rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf p-plan:isInputVarOf ;
              rdfs:domain :Variable ;
              rdfs:range :ExperimentStep ;
              rdfs:comment "A input variable of the step."@en ;
              rdfs:label "is input variable of"@en .


###  http://qptdat.plasma-mds.org/ontology/isOutputVarOf
:isOutputVarOf rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf p-plan:isOutputVarOf ;
               rdfs:domain :Variable ;
               rdfs:range :ExperimentStep ;
               rdfs:comment "A output variable of the step."@en ;
               rdfs:label "is output variable of"@en .


###  http://qptdat.plasma-mds.org/ontology/isSubConfigurationOf
:isSubConfigurationOf rdf:type owl:ObjectProperty ;
                      rdfs:domain :Configuration ;
                      rdfs:range :Configuration ;
                      rdfs:comment "A more specific configuration."@en ;
                      rdfs:label "is sub configuration of"@en .


###  http://qptdat.plasma-mds.org/ontology/isSubExperimentStepOf
:isSubExperimentStepOf rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf p-plan:isSubPlanOfPlan ;
                       rdfs:domain :ExperimentStep ;
                       rdfs:range :PlasmaStudy ;
                       rdfs:comment "A more detailed step as part of the plasma study."@en ;
                       rdfs:label "is sub experiment step of"@en .


###  http://qptdat.plasma-mds.org/ontology/isSubMaterialPropertyOf
:isSubMaterialPropertyOf rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf owl:topObjectProperty ;
                         rdfs:domain :MaterialProperty ;
                         rdfs:range :MaterialProperty ;
                         rdfs:comment "A more specific material property than the material property."@en ;
                         rdfs:label "is sub material property of"@en .


###  http://qptdat.plasma-mds.org/ontology/isSubPlasmaStudyTagOf
:isSubPlasmaStudyTagOf rdf:type owl:ObjectProperty ;
                       rdfs:domain :PlasmaStudyTag ;
                       rdfs:range :PlasmaStudyTag ;
                       rdfs:comment "A tag that is a subtopic of the plasma study tag."@en ;
                       rdfs:label "is sub plasma study tag of"@en .


###  http://qptdat.plasma-mds.org/ontology/isSubResearchTopicOf
:isSubResearchTopicOf rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf owl:topObjectProperty ;
                      rdfs:domain :ResearchTopic ;
                      rdfs:range :ResearchTopic ;
                      rdfs:comment "A research topic that is a subtopic of the research topic."@en ;
                      rdfs:label "is sub research topic of"@en .


###  http://qptdat.plasma-mds.org/ontology/isSupplementing
:isSupplementing rdf:type owl:ObjectProperty ;
                 rdfs:domain :Dataset ;
                 rdfs:range :Publication ;
                 rdfs:comment "A publication related to the dataset."@en ;
                 rdfs:label "is supplementing"@en .


###  http://qptdat.plasma-mds.org/ontology/license
:license rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf dcterms:license ;
         rdfs:domain :Dataset ;
         rdfs:range :LicenseDocument ;
         rdfs:comment "A legal document regulating the usage of the dataset."@en ;
         rdfs:label "license"@en .


###  http://qptdat.plasma-mds.org/ontology/measuredIn
:measuredIn rdf:type owl:ObjectProperty ;
            rdfs:domain :Configuration ;
            rdfs:range :MeasurementUnit ;
            rdfs:comment "The measurement unit of the configuration."@en ;
            rdfs:label "measured in"@en .


###  http://qptdat.plasma-mds.org/ontology/usesDevice
:usesDevice rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain :AdjustedDevice ;
            rdfs:range :Device ;
            rdfs:comment "The link between a device to another device with already existing settings. This property should enable reusing of devices already connected to specific configurations."@en ;
            rdfs:label "uses device"@en .


###  http://qptdat.plasma-mds.org/ontology/usesMedium
:usesMedium rdf:type owl:ObjectProperty ;
            rdfs:domain :MediumConfiguration ;
            rdfs:range :Medium ;
            rdfs:comment "The link between the medium configuration of a plasma source and a medium."@en ;
            rdfs:label "uses medium"@en .


###  http://qptdat.plasma-mds.org/ontology/wasDevelopedBy
:wasDevelopedBy rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf <http://www.w3.org/ns/prov#wasAttributedTo> ;
                rdfs:domain :Source ;
                rdfs:range <http://www.w3.org/ns/prov#Agent> ;
                rdfs:comment "The link of an agent involved in the developement to the plasma source."@en ;
                rdfs:label "developed by"@en .


###  http://www.w3.org/ns/prov#wasAttributedTo
<http://www.w3.org/ns/prov#wasAttributedTo> rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://qptdat.plasma-mds.org/ontology/lowerLimit
:lowerLimit rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf <http://www.w3.org/ns/prov#value> ;
            rdfs:domain :Range ;
            rdfs:range xsd:decimal ;
            rdfs:comment "The lower limit of the range, including the value. "@en ;
            rdfs:label "lower limit"@en .


###  http://qptdat.plasma-mds.org/ontology/publicationDate
:publicationDate rdf:type owl:DatatypeProperty ;
                 rdfs:domain <http://www.w3.org/ns/dcat#Dataset> ;
                 rdfs:range xsd:date ;
                 rdfs:comment "A date of publication of the dataset."@en ;
                 rdfs:label "publication date"@en .


###  http://qptdat.plasma-mds.org/ontology/studyAbstract
:studyAbstract rdf:type owl:DatatypeProperty .


###  http://qptdat.plasma-mds.org/ontology/studyTitle
:studyTitle rdf:type owl:DatatypeProperty .


###  http://qptdat.plasma-mds.org/ontology/upperLimit
:upperLimit rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf <http://www.w3.org/ns/prov#value> ;
            rdfs:domain :Range ;
            rdfs:range xsd:decimal ;
            rdfs:comment "The upper limit of the range, including the value."@en ;
            rdfs:label "upper limit"@en .


###  http://qptdat.plasma-mds.org/ontology/value
:value rdf:type owl:DatatypeProperty ;
       rdfs:subPropertyOf <http://www.w3.org/ns/prov#value> ;
       rdfs:domain :NumericalConfiguration ;
       rdfs:range xsd:decimal ;
       rdfs:comment "The numerical value of the configuration."@en ;
       rdfs:label "value"@en .


###  http://www.w3.org/ns/prov#value
<http://www.w3.org/ns/prov#value> rdf:type owl:DatatypeProperty .


